{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "QoreDB/QoreDB.csproj",
            "QoreDB.Tui/QoreDB.Tui.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appName": "",
      "_appTitle": "QoreDB Documentation",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/logo.svg"
    },
    "template": [
      "default",
      "modern"
    ]
  }
}